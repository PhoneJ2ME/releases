<configuration>
<properties>
    <property Key="microedition.smartcardslots"
        Value="0C,1H"
        Scope="system"
        Comment="Smart card slot identifiers."/>

    <property Key="com.sun.midp.io.j2me.apdu.satselectapdu"
        Value="00.a4.04.00.07.a0.00.00.00.62.3.1.7F"
        Scope="internal"
        Comment="APDU command for selecting SIM application."/>

    <property Key="com.sun.satsa.store_csr_list"
        Value="true"
        Scope="internal"
        Comment="Save persistent copy of CSR public keys."/>

    <property Key="com.sun.satsa.keygen"
        Value="true"
        Scope="internal"
        Comment="Internal flag to identify key generation."/>

    <property Key="com.sun.satsa.certsig"
        Value="true"
        Scope="internal"
        Comment="Internal flag to identify certificate signing."/>

    <property Key="microedition.satsa.crypto.version"
        Value="1.0"
        Scope="system"
        Comment="Version of SATSA-CRYPTO API supported by the device."/>

    <property Key="microedition.satsa.apdu.version"
        Value="1.0"
        Scope="system"
        Comment="Version of SATSA-APDU API supported by the device."/>

    <property Key="microedition.satsa.pki.version"
        Value="1.0"
        Scope="system"
        Comment="Version of SATSA-PKI API supported by the device."/>

</properties>
</configuration>
